{
  "name": "@mochila/n8n-nodes-jetnet",
  "version": "0.1.2",
  "description": "Enterprise-grade n8n integration nodes for JetNet Aviation API - Comprehensive aircraft data, market intelligence, and AI-powered workflow automation",
  "keywords": [
    "n8n",
    "n8n-node",
    "n8n-community-node",
    "n8n-community-node-package",
    "nodemation",
    "workflow",
    "workflow-automation",
    "jetnet",
    "aviation",
    "aviation-api",
    "aircraft",
    "aircraft-data",
    "market-intelligence",
    "api-integration",
    "typescript",
    "ai-tools",
    "langchain",
    "automation",
    "enterprise-integration",
    "aviation-industry",
    "flight-data",
    "aerospace"
  ],
  "license": "MIT",
  "homepage": "https://github.com/mochila-ai/jetnet",
  "author": "Matt Busi",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mochila-ai/jetnet.git"
  },
  "bugs": {
    "url": "https://github.com/mochila-ai/jetnet/issues"
  },
  "main": "index.js",
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc && npm run copy-assets",
    "copy-assets": "mkdir -p dist/nodes/JetNet dist/nodes/JetNetTool && cp nodes/JetNet/*.svg dist/nodes/JetNet/ 2>/dev/null; cp nodes/JetNetTool/*.svg dist/nodes/JetNetTool/ 2>/dev/null || true",
    "dev": "tsc --watch",
    "format": "prettier --write '**/*.ts'",
    "lint": "eslint .",
    "lintfix": "eslint . --fix",
    "test": "jest",
    "prepublishOnly": "npm run build && npm run lint"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/JetNetApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/JetNet/JetNet.node.js",
      "dist/nodes/JetNetTool/JetNetTool.node.js"
    ]
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "jest": "^30.1.3",
    "n8n-core": "^1.19.0",
    "n8n-workflow": "^1.19.0",
    "prettier": "^3.1.0",
    "ts-jest": "^29.4.1",
    "typescript": "^5.9.2"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
