      "typeVersion": 1,
      "position": [450, 200],
      "credentials": {
        "jetNetApi": {
          "id": "1",
          "name": "JetNet API"
        }
      }
    },
    {
      "parameters": {
        "resource": "company",
        "operation": "getCompanyById",
        "companyId": "{{ $json.items[0].companyId }}",
        "securityToken": "{{ $credentials.jetNetApi.apiToken }}"
      },
      "name": "Get Company Details",
      "type": "n8n-nodes-jetnet.jetNet",
      "typeVersion": 1,
      "position": [650, 200],
      "credentials": {
        "jetNetApi": {
          "id": "1",
          "name": "JetNet API"
        }
      }
    },
    {
      "parameters": {
        "resource": "company",
        "operation": "getCompanyContacts",
        "companyId": "{{ $json.items[0].companyId }}",
